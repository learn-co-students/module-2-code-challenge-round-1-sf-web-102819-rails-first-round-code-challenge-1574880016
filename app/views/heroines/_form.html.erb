<%= form_for @heroine do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br /><br />

  <%= f.label :super_name %>:
  <%= f.text_field :super_name %><br /><br />

  <%= f.label "Select Power" %>:
  <%= f.collection_select :power_id, Power.all, :id, :name %>

<br><br>
  <%= f.submit %>
<% end %>



   <% if flash[:errors] %>
        <% flash[:errors].each do |error| %>
        <p><%= error %></p> <br>
    <% end %>
    <% end %>