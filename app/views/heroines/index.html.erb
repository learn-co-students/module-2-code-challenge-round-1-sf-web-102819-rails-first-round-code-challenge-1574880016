<h2>All Heroines</h2>

<h3> Search:</h3>
<%= form_tag(heroines_path, method: :get) do%>
<%= text_field_tag(:search, params[:search])%>
<%= submit_tag "Seach for it"%>
<% end %>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
