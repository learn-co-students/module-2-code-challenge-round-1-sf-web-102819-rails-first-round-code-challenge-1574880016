<h1> Add Your Own Super-Heroine! </h1>
<% if @errors %>
    <ol>
     <% @errors.each do |error| %>
       <li> <%= error %> </li>
    <% end %>
    </ol>
    <%end %>
<%= form_for @heroine do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name %> <br>

    <%=f.label :super_name %>
    <%=f.text_field :super_name %> <br>

    <%=f.label :power %>
    <%=f.collection_select :power_id, Power.all, :id, :name %> <br>

    <%=f.submit %>
<% end %>