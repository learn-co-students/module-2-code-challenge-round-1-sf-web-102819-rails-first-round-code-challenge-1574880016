<h1>Create Heroine:</h1><br>
<% if @heroine.valid? != true %>
    <%= flash[:error].to_s %>
    <%end%>
  
<%= form_for @heroine do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %><br>
    

    
    <%= f.label :power_id %><br>
    
    <%= f.collection_select :power_id, Power.all, :id, :name %>
    
    <%= f.submit %>
    

<% end %>




